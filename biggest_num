max_len = 5
    numbers = list(map(str, numbers))
    new_numbers = []
    for idx, num in enumerate(numbers) :
        if len(num) < max_len :
            num = num*(max_len//len(num)) + num[:max_len%len(num)]
        else : pass
        new_numbers.append([idx, num])
    
    new_numbers = sorted(new_numbers, key=lambda x : x[1], reverse=True)
    answer = ""
    for n in new_numbers :
        answer += numbers[n[0]]
    return answer
