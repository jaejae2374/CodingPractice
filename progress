def solution(progresses, speeds):
    f = lambda x, y : x+y
    answer = []

    while progresses :
        progresses = list(map(f, progresses, speeds))
        cnt=0
        while progresses[0] >= 100 :
            cnt+=1
            del progresses[0]
            del speeds[0]
            if not progresses : break
                
        if cnt != 0 :
            answer.append(cnt)
    return answer
