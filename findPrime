from itertools import permutations

def solution(numbers):
    
    answer = []
    cnt = len(numbers)
    
    for i in range(1, cnt+1) : 
        for j in list(permutations(numbers, i)) :
            num = int("".join(j))
            if num < 2 : continue
            for k in range(2, int(num/2)+1) :
                if num%k==0 : break
            else : answer.append(num)
    answer = set(answer)
    return len(answer)
